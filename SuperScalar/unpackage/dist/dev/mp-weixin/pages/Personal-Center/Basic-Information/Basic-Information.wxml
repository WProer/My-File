<view class="Basic-Information"><form class="Basic-Information_form" action><view class="NAME form-item_input"><text>真实姓名:</text><input type="text" name id required="{{true}}"/></view><view class="SEX form-item_input"><text>性别:</text><radio-group name><label><radio value="保密"></radio><text>保密</text></label><label style="margin-left:10px;"><radio value="男"></radio><text>男</text></label><label style="margin-left:10px;"><radio value="女"></radio><text>女</text></label></radio-group></view><view class="EMAIL form-item_input"><text>电子邮箱:</text><input type="email" name id/></view><view class="PHONE form-item_input"><text>电话号码:</text><input type="number" name id required="{{true}}"/></view><view class="QQ form-item_input"><text>QQ:</text><input type="number" name id required="{{true}}"/></view><view class="ADDRESS form-item_input"><text>所在地址:</text><picker name id class="_select"><view value="" class="_option">省级地区</view><block wx:for="{{province}}" wx:for-item="item" wx:for-index="index"><view value="{{index}}" class="_option">{{item.ProvinceName}}</view></block></picker><picker style="margin-left:10px;" name id class="_select"><view value="" class="_option">市级地区</view></picker><picker style="margin-left:10px;" name id class="_select"><view value="" class="_option">县/区</view></picker></view><view class="POSTAL——ADD form-item_input"><text>通讯地址:</text><input type="text" name id required="{{true}}"/></view><view class="POSTAL——CODE form-item_input"><text>邮编:</text><input type="text" name id required="{{true}}"/></view><view class="BIRTHDAY form-item_input"><text>出生年月:</text><picker mode="date" value="{{date}}" start="{{startDate}}" end="{{endDate}}" required="{{true}}" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><view>{{date}}</view></picker></view><view class="TRADE form-item_input"><text>从事行业:</text><picker name id required="{{true}}" class="_select"><view value="" class="_option">请选择你所从事的行业</view><block wx:for="{{trade}}" wx:for-item="item" wx:for-index="index"><view value="{{index}}" class="_option">{{item.name}}</view></block></picker></view><view class="BUTTON form-item_input"><button form-type="submit">提交</button><button form-type="reset">重置</button></view></form></view>