.register-content {
  width: 100%;
  height: 100vh;
  padding: 10px 30px;
  overflow: hidden;
  box-sizing: border-box;
  background-color: #fff;
  position: relative;
}
.register-content .register-logo {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  overflow: hidden;
  box-sizing: border-box;
  margin-top: 200px;
}
.register-content .register-logo .logo {
  flex-shrink: 0;
  width: 100px;
  height: 100px;
  overflow: hidden;
  box-sizing: border-box;
}
.register-content .register-logo .logo image {
  width: 100%;
  height: 100%;
  overflow: hidden;
  box-sizing: border-box;
}
.register-content .register-logo text {
  flex-shrink: 0;
  color: #9999FF;
  font-family: STXingkai;
  font-size: 50px;
  font-weight: 700;
  text-shadow: 6px -2px 3px #bdd8ff;
}
.register-content .catchphrase {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  overflow: hidden;
  box-sizing: border-box;
  margin: 30px 0px;
}
.register-content .catchphrase text {
  color: #9999FF;
  font-weight: 700;
  font-size: 25px;
  font-style: italic;
  text-shadow: 6px -2px 3px #bdd8ff;
}
.register-content .information {
  width: 100%;
  overflow: hidden;
  box-sizing: border-box;
  padding: 10px;
}
.register-content .information .information-item {
  width: 100%;
  padding: 10px 30px;
  display: flex;
  align-items: center;
  overflow: hidden;
  box-sizing: border-box;
}
.register-content .information .information-item input {
  margin-left: 10px;
  width: 100%;
  padding: 0px 5px;
  box-sizing: border-box;
  overflow: hidden;
  border-radius: 5px;
  background-color: #c2f1ff;
}
.register-content .register-button {
  width: 100%;
  overflow: hidden;
  box-sizing: border-box;
  padding: 20px 15px;
  display: flex;
  align-items: center;
}
.register-content .register-button button {
  color: #ffffff;
  background-color: #99CCFF;
  font-weight: 700;
}
.register-content .register_Other_methods {
  width: 100%;
  overflow: hidden;
  box-sizing: border-box;
  padding: 10px 30px;
  text-align: center;
  position: absolute;
  bottom: 10px;
  left: 0px;
  right: 0px;
}
.register-content .register_Other_methods text {
  width: 100%;
  font-size: 10px;
  font-weight: 700;
  color: #a7a7a7;
}
.register-content .register_Other_methods text::after {
  content: "————————";
  position: absolute;
  width: 100px;
  height: 1px;
  color: #ccc;
  top: 13px;
  bottom: 10px;
  right: 55px;
}
.register-content .register_Other_methods text::before {
  content: "————————";
  position: absolute;
  width: 100px;
  height: 1px;
  color: #ccc;
  top: 13px;
  bottom: 10px;
  left: 55px;
}
.register-content .register_Other_methods .other_content {
  width: 100%;
  overflow: hidden;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 8px;
}
.register-content .register_Other_methods .other_content .other-content_item {
  width: 30px;
  height: 30px;
  overflow: hidden;
  box-sizing: border-box;
  border: 1px solid #ccc;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 10px 10px;
  border-radius: 50%;
}
.register-content .register_Other_methods .other_content .other-content_item image {
  width: 25px;
  height: 25px;
  overflow: hidden;
  box-sizing: border-box;
}

